<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.botanju.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.techgirls.myapplication.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.techgirls.myapplication.activity.HomeActivity"/>
        <activity android:name="com.techgirls.myapplication.activity.ListaContatosActivity"/>
        <activity android:name="com.techgirls.myapplication.activity.TransferenciaActivity"/>
        <activity android:name="com.techgirls.myapplication.activity.ComprovanteActivity"/>
    </application>

</manifest>